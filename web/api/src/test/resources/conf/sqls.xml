<?xml version="1.0" encoding="UTF-8"?>
<sqls xmlns="http://www.leapframework.org/schema/sqls">
	
	<command key="security.findUserDetails">
		select * from User where id = :userId
	</command>
    
    <command key="security.findUserDetailsByUsername">
        select * from User where loginName = :username
    </command>    

</sqls>