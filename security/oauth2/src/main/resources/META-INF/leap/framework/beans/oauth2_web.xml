<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.leapframework.org/schema/beans" default-lazy-init="true">

    <bean type="leap.oauth2.web.OAuth2WebConfig" 
    	  class="leap.oauth2.web.DefaultOAuth2WebConfig" primary="true">
          
          <additional-type-def type="leap.oauth2.web.OAuth2WebConfigurator" primary="true"/>
          
    </bean>          
    
    <bean type="leap.oauth2.web.token.WebAccessTokenManager"
          class="leap.oauth2.web.token.DefaultWebAccessTokenManager" primary="true"/>
    
    <bean type="leap.oauth2.web.auth.OAuth2WebResponseHandler"
          class="leap.oauth2.web.auth.DefaultOAuth2WebResponseHandler" primary="true"/>
          
    <bean name="jdbc"
          type="leap.oauth2.web.token.WebAccessTokenStore"
          class="leap.oauth2.web.token.JdbcWebAccessTokenStore"/>          
          
    <bean type="leap.web.security.SecurityInterceptor" class="leap.oauth2.web.auth.OAuth2WebSecurityInterceptor"/>
</beans>